#include<queue>
vector<int> kSmallest(int *input, int n, int k) {
	// Write your code here
    priority_queue<int>pq;
for(int i=0;i<k;i++)
{
    pq.push(input[i]);
}
    for(int i=k;i<n;i++)
    {
        if(pq.top()>input[i])
        {pq.pop();
        pq.push(input[i]);}
    }
    vector<int>t;
    for(int i=0;i<k;i++)
    {
       t.push_back(pq.top());
        pq.pop();
    }
    return t;
}
